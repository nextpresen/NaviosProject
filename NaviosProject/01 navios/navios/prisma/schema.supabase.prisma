generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("SUPABASE_DATABASE_URL")
}

model Event {
  id          String   @id @default(cuid())
  title       String
  content     String
  author_id   String?
  latitude    Float
  longitude   Float
  event_date  DateTime
  expire_date DateTime
  event_image String
  created_at  DateTime @default(now())
  updated_at  DateTime @updatedAt
}
